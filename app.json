{
  "expo": {
    "name": "my-app1",
    "slug": "my-app1",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "https://i.pinimg.com/1200x/df/1e/1e/df1e1efb857c7e0608c09ab9d908bf1d.jpg",
    "scheme": "my movie App",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "https://i.pinimg.com/1200x/df/1e/1e/df1e1efb857c7e0608c09ab9d908bf1d.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "https://i.pinimg.com/1200x/df/1e/1e/df1e1efb857c7e0608c09ab9d908bf1d.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "https://i.pinimg.com/1200x/df/1e/1e/df1e1efb857c7e0608c09ab9d908bf1d.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
